{
    "name": "knplabs/console-service-provider",
    "type": "library",
    "description": "console service provider for Silex",
    "keywords": [ "console", "silex" ],
    "homepage": "http://knplabs.com",
    "license": "MIT",
    "authors": [
        {
            "name":     "KnpLabs",
            "homepage": "http://knplabs.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/console": "^5.0",
        "silex/silex": "2.5.x-dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "twig/twig": "~3.0",
        "symfony/twig-bridge": "^5.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/monolog-bridge": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Knp\\": "Knp/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Knp\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GregOriol/Silex"
        }
    ]
}
